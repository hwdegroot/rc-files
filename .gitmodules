[submodule ".vim/bundle/Vundle.vim"]
  path = .vim/bundle/Vundle.vim
  url = git@github.com:VundleVim/Vundle.vim.git
  ignore = dirty
[submodule "gittools"]
  path = gittools
  url = git@github.com:hwdegroot/gittools.git
  ignore = dirty
[submodule "fonts"]
  path = fonts
